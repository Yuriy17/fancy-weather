{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "extends": "airbnb-base",
  "rules": {
    "max-len": [1, 130, 2, {
      "ignoreComments": true
    }],
    "camelcase": "warn"
  },
  "globals": {
    "fetch": false
  }
}